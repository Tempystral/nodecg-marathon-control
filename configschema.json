{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {
            "websocket": {
                "ip": "localhost",
                "port": "4455",
                "password": ""
            },
            "baseRtmpUrl": "https://rtmp.example.com/hls/",
            "botToken": "",
            "checklist": {}
    },
    "title": "Root Schema",
    "required": [
        "websocket",
        "baseRtmpUrl",
        "botToken",
        "checklist"
    ],
    "properties": {
        "websocket": {
            "type": "object",
            "default": {},
            "title": "The websocket Schema",
            "required": [
                "ip",
                "port",
                "password"
            ],
            "properties": {
                "ip": {
                    "type": "string",
                    "default": "",
                    "title": "The ip Schema",
                    "examples": [
                        "localhost"
                    ]
                },
                "port": {
                    "type": "string",
                    "default": "",
                    "title": "The port Schema",
                    "examples": [
                        "4455"
                    ]
                },
                "password": {
                    "type": "string",
                    "default": "",
                    "title": "The password Schema",
                    "examples": [
                        ""
                    ]
                }
            },
            "examples": [{
                "ip": "localhost",
                "port": "4455",
                "password": ""
            }]
        },
        "baseRtmpUrl": {
            "type": "string",
            "default": "",
            "title": "The baseRtmpUrl Schema",
            "examples": [
                "https://rtmp.example.com/hls/"
            ]
        },
        "botToken": {
            "type": "string",
            "default": "",
            "title": "The botToken Schema",
            "examples": [
                ""
            ]
        },
        "checklist": {
            "type": "object",
            "default": {},
            "title": "The checklist Schema",
            "required": [],
            "properties": {},
            "examples": [{}]
        }
    },
    "examples": [{
        "websocket": {
            "ip": "localhost",
            "port": "4455",
            "password": ""
        },
        "baseRtmpUrl": "https://rtmp.example.com/hls/",
        "botToken": "",
        "checklist": {}
    }]
}